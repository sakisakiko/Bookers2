<h1>user一覧</h1>
<div class="continer">
  <div class="row">
　　 <div class="col-m-8">
　　     <h2>Users</h2>
　　     <table class="table">
　　     <tr>
　　       <th>image</th>
　　       <th>name</th>
　　       <th></th>
　　     </tr>
　　     
　　     <% @users.each do |user| %>
　　     <tr>
　　       <th><%=image_tag user.get_profile_image(80,80) %></th>
　　       <th><%=user.name %></th>
　　       <th><%=link_to 'Show',user_path(user.id) %></th>
　　       <!--下一行不要後で消す-->
　　        <th><%=link_to 'Destroy',user_path(user.id),method: :delete %></th>
　　     </tr>
　　    <% end %>
　　   
　　   </table>　
     </div>
  </div>
</div>
